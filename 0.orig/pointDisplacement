FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    AMI1
    {
        //type            fixedValue;
        type            cyclicAMI;
        value           uniform (0 0 0);
    }

    AMI2
    {
        //type            calculated;
        type            cyclicAMI;
        value           uniform (0 0 0);
    }

    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    outlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    fan 
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    ".*"
    {
      type  fixedValue;
      value uniform (0 0 0);
    }
}


